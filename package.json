{
  "name": "vueplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "app": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "wt new-tab cmd /k npm run app; cmd /k npm run server; new-tab",
    "lint": "vue-cli-service lint",
    "server": "nodemon src/webserver.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "nprogress": "^0.2.0",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.0",
    "vue": "^2.6.11",
    "vue-router": "^3.5.1",
    "vue-socket.io": "^3.0.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-useless-escape": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
